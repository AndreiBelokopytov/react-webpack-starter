{
  "name": "react-webapck-starter",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "git+https://github.com/AndreiBelokopytov/react-webpack-starter",
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server",
