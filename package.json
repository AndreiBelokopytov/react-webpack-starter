    "postcss-loader": "^2.0.6",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist"
